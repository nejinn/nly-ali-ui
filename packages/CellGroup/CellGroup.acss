.nly-cell-group {
  background-color: #fff;
}

.nly-cell-group__title {
  padding: 30rpx 20rpx 20rpx 20rpx;
  color: #969799;
  font-size: 35rpx;
  line-height: 35rpx;
}

[class*='nly-hairline']::after {
  position: absolute;
  box-sizing: border-box;
  content: ' ';
  pointer-events: none;
  top: -50%;
  right: -50%;
  bottom: -50%;
  left: -50%;
  border: 0 solid #ebedf0;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
}